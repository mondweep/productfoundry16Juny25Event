# Server Configuration\nPORT=3001\nNODE_ENV=development\nAPI_VERSION=v1\n\n# Database\nMONGODB_URI=mongodb://localhost:27017/live_conditions\nDB_NAME=live_conditions\n\n# JWT Configuration\nJWT_SECRET=your-super-secret-jwt-key-here\nJWT_REFRESH_SECRET=your-refresh-secret-here\nJWT_EXPIRE=24h\nJWT_REFRESH_EXPIRE=7d\n\n# External APIs\nWEATHER_API_KEY=your-weather-api-key\nOCEAN_API_KEY=your-ocean-conditions-api-key\nTRAFFIC_API_KEY=your-traffic-api-key\n\n# Weather Service URLs\nMETSERVICE_NZ_API=https://api.metservice.com\nBOM_AU_API=https://api.bom.gov.au\nOPENWEATHER_API=https://api.openweathermap.org/data/2.5\n\n# Cache Configuration\nREDIS_URL=redis://localhost:6379\nCACHE_TTL=300\n\n# Rate Limiting\nRATE_LIMIT_WINDOW=15\nRATE_LIMIT_MAX_REQUESTS=100\n\n# CORS\nCORS_ORIGIN=http://localhost:3000\n\n# Logging\nLOG_LEVEL=info\nLOG_FILE=./logs/app.log\n\n# WebSocket\nWS_PORT=3002\nWS_HEARTBEAT_INTERVAL=30000\n\n# Security\nBCRYPT_ROUNDS=12\nAPI_KEY_HEADER=x-api-key\n\n# Monitoring\nHEALTH_CHECK_ENDPOINT=/health\nMETRICS_ENDPOINT=/metrics